# Google Calendar API Configuration
# Get these from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/callback/google

# Google OAuth Refresh Token (get this after completing OAuth flow)
GOOGLE_REFRESH_TOKEN=your_google_refresh_token_here

# Google Calendar ID (usually 'primary' for personal calendar)
GOOGLE_CALENDAR_CALENDAR_ID=primary

# OpenAI API Configuration
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
NODE_ENV=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Optional: Database Configuration (if you add a database later)
# DATABASE_URL=your_database_connection_string_here

# Optional: Email Configuration (if you add email functionality)
# SMTP_HOST=your_smtp_host
# SMTP_PORT=587
# SMTP_USER=your_smtp_username
# SMTP_PASS=your_smtp_password
